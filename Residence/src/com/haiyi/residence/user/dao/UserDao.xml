<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
  <mapper namespace="com.haiyi.residence.user.dao.UserDao">  
  <select id="getAll"  resultType="com.haiyi.residence.model.User">  
      SELECT * FROM user 
  </select>
  <insert id="insertUser" parameterType="com.haiyi.residence.model.User">  
        insert into user(id,name,age) values(#{id},#{name},#{age})  
  </insert> 
</mapper> 
